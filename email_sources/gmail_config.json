{
  "target_domains": ["snowflake.com"],
  "max_emails_per_run": 10,
  "processing_interval_minutes": 5,
  "s3_bucket": "your-email-processing-bucket",
  "aws_access_key": "your_aws_access_key",
  "aws_secret_key": "your_aws_secret_key", 
  "aws_region": "us-east-1",
  "snowflake_user": "SPAWAR",
  "snowflake_password": "your_password",
  "snowflake_account": "SFSEAPAC-SPAWAR_AWSEAST1",
  "snowflake_warehouse": "COMPUTE_WH",
  "email_filters": {
    "include_keywords": ["meeting", "urgent", "action", "deadline", "project", "client", "demo"],
    "exclude_keywords": ["newsletter", "unsubscribe", "marketing", "promotional"],
    "date_range_days": 7,
    "include_attachments": true
  },
  "processing_options": {
    "enable_urgent_alerts": true,
    "enable_duplicate_detection": true,
    "max_content_length": 50000,
    "enable_sentiment_analysis": true,
    "enable_entity_extraction": false
  },
  "monitoring": {
    "enable_metrics_logging": true,
    "alert_thresholds": {
      "processing_latency_seconds": 300,
      "error_rate_percentage": 5,
      "queue_depth": 100
    }
  }
}
